<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.nectarine.uaspangyo" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".kr.nectarine.uaspangyo.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".kr.nectarine.uaspangyo.activity.DetailActivity"
            android:label="@string/title_activity_detail"
            android:parentActivityName=".kr.nectarine.uaspangyo.activity.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="kr.nectarine.uaspangyo.kr.nectarine.uaspangyo.activity.MainActivity" />
        </activity>
    </application>

</manifest>
